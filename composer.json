{
    "name": "omatamix/laravel-ban",
    "description": "This package provides the necessary tools needed to ban and unban eloquent models.",
    "keywords": [],
    "license": "MIT",
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Omatamix\\LaravelBan\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omatamix\\LaravelBan\\BanServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
